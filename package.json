        "express": "^5.1.0",
        "express-rate-limit": "^7.5.0",
        "express-session": "^1.18.1",
        "gsap": "^3.13.0",
        "morgan": "^1.10.0",
        "node-cron": "^3.0.3",
        "opentype.js": "^1.3.4",
        "path": "^0.12.7",
        "redis": "^5.0.1",
        "sharp": "^0.34.2",
        "socks-proxy-agent": "^8.0.5",
        "uuid": "^11.1.0",
        "zod": "^3.25.42"
