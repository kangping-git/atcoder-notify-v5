{
    "name": "atcoder_notify_v5",
    "version": "0.0.0-alpha",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build:frontend": "cd frontend && npm run build",
        "build:scraping": "cd scraping && npm run build",
        "build:backend": "cd backend && npm run build",
        "build:judge": "cd judgement/judge && npm run build",
        "build": "npm run build:frontend && npm run build:scraping && npm run build:backend && npm run build:judge",
        "build:frontend:release": "cd frontend && npm run build:release",
        "build:scraping:release": "cd scraping && npm run build:release",
        "build:release": "npm run build:frontend:release && npm run build:scraping:release && npm run build:backend",
        "build:frontend:watch": "cd frontend && npm run build:watch",
        "build:scraping:watch": "cd scraping && npm run build:watch",
        "build:backend:watch": "cd backend && npm run build:watch",
        "build:judge:watch": "cd judgement/judge && npm run build:watch",
        "build:watch": "concurrently \"npm run build:frontend:watch\" \"npm run build:scraping:watch\" \"npm run build:backend:watch\" \"npm run build:judge:watch\"",
        "docker:build": "docker build -t atcoder-notify-v5 .",
        "docker:build:scraping": "docker build -f scraping/Dockerfile -t atcoder-notify-v5-scraping .",
        "docker:build:all": "npm run docker:build && npm run docker:build:scraping",
        "docker:run": "docker run --restart=always -d --name atcoder-notify-v5 -p 4080:4080 atcoder-notify-v5",
        "docker:run:scraping": "docker run --restart=always -d --name atcoder-notify-v5-scraping -p 4081:4081 atcoder-notify-v5-scraping",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:reset": "prisma migrate reset",
        "db:studio": "prisma studio",
        "db:migrate": "prisma migrate deploy",
        "start": "node backend/web/dist/index.js"
    },
    "keywords": [
        "AtCoder",
        "Scraping"
    ],
    "author": "Kangping",
    "license": "MIT",
